{
  "manifest_version": 2,
  "name": "Cliente APH",
  "description": "Extenção para lançamento de horas do APH EGS",
  "version": "0.3.0",
  "browser_action": {
    "default_icon": {
      "19": "images/calendar_icon.png",
      "38": "images/calendar_icon.png"
    },
    "default_title": "Cliente APH",
    "default_popup": "index.html"
  },
  "permissions": [
                  "cookies",
                  "storage",
                  "*://aph.egs.com.br/*"
                 ]  
}